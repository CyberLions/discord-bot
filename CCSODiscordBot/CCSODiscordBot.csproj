<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <DockerComposeProjectPath>../docker-compose.dcproj</DockerComposeProjectPath>
    <UserSecretsId>db06b900-0a3e-4947-a2e9-f8783a173c61</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Discord.Net" />
    <None Remove="Modules\" />
    <None Remove="Modules\Memes\" />
    <None Remove="Modules\Roles\" />
    <None Remove="Modules\CTFd\" />
    <None Remove="Microsoft.Extensions.DependencyInjection" />
    <None Remove="Services\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Discord.Net" Version="3.6.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.UserSecrets" Version="6.0.1" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.0" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Modules\" />
    <Folder Include="Modules\Memes\" />
    <Folder Include="Modules\Roles\" />
    <Folder Include="Modules\CTFd\" />
    <Folder Include="Services\" />
  </ItemGroup>
</Project>
